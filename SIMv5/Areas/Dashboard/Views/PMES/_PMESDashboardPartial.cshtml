@model DevExpress.DashboardWeb.Mvc.DashboardSourceModel
@Html.DevExpress().DashboardViewer(settings =>
{
    settings.Name = "PMESViewer";
    settings.CallbackRouteValues = new { Controller = "PMES", Action = "PMESDashboardPartial", Area = "Dashboard", v = ViewBag.Vigencia, e = ViewBag.Encuesta, t = ViewBag.IdTercero };
    settings.ExportRouteValues = new { Controller = "PMES", Action = "PMESDashboardPartialExport", Area = "Dashboard", v = ViewBag.Vigencia, e = ViewBag.Encuesta, t = ViewBag.IdTercero };
    settings.FullscreenMode = true;
    settings.AllowExportDashboard = true;
}).BindToSource(Model).GetHtml()