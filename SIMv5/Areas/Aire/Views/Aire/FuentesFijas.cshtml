    function consultarJsonDetalle(grupo, div, id) {

        onLoad("tabInfo");
        $.ajax({
            type: "POST",
            url: '@Url.Action("consultarJsonDetallegrupo", "Aire")',
            data: { idItem: $("#txtidEstadoBase").val(), form: $("#txtForm").val(), idgrupo: grupo, tblEstados: $("#txttblEstados").val(), copia: (tipoV == 'D')},
            beforeSend: function () { },
            success: function (response) {
                if (response != "[]") {
                    jsonDetalle = eval('(' + response + ')');
                    var html = consultarDetalle(jsonDetalle, id, 0, "acordionGeneracionPrincipal_" + id);

                    $("#acordionDetallePrincipal" + id).remove();
                    $("#" + div).append(html);

                } else {

                }

                offLoad("tabInfo");

            }
        });
    }
