    var jsonInfoGeneral;




    function CrearEstadoItem() {

        $.ajax({
            type: "POST",
            url: '@Url.Action("CrearEstadoItem", "Aire")',
            data: { idItem: $("#txtidItem").val(), idFormulario: $("#txtForm").val(), idVisita: $("#txtidVisita").val(), instalacion: $("#txtinstalacion").val(), tercero: $("#txttercero").val() },
            beforeSend: function () { },
            success: function (response) {
                var dato = eval('(' + response + ')');
                $("#txtidEstadoBase").val(dato)
            }
        });
    }

    function GuardarInformacionItem() {
        onLoad("tabMapa");
        jsonInfoGeneral.NOMBRE = $("#txtNombreOficial").text();
        jsonInfoGeneral.X = $("#txtX").val() * 1;
        jsonInfoGeneral.Y = $("#txtY").val() * 1;
        var jsonOficial = JSON.stringify(jsonInfoGeneral)

        $.ajax({
            type: "POST",
            url: '@Url.Action("GuardarInformacionItem", "Aire")',
            data: { jsonInfo: jsonOficial, idFormulario: $("#txtForm").val() },
            beforeSend: function () { },
            success: function (response) {
                if (response == "Ok") {
                    mensajeAlmacenamiento("Almacenamiento Exitoso");
                    $("#Tab2").css('display', 'block');
                    $("#Tab3").css('display', 'block');
                    $("#Tab4").css('display', 'block');

                    $("#Tab6").css('display', 'block');
                    $("#Tab7").css('display', 'block');
                    offLoad("tabMapa");

                    if ((tipoV == "N" && $("#txtidEstadoBase").val() == "-1") || tipoV == "D") {
                        CrearEstadoItemUni();
                    }
                } else {
                    mensajeAlmacenamiento("Error al almacenar la informacion")
                    offLoad("tabMapa");
                }
            }
        });
    }


        function GuardarInformacionCaracteristica(id) {
            if (id == 0)
                $('#btnGuardar2').attr('disabled', true);
            else if (id == 1)
                $('#btnGuardar3').attr('disabled', true);

        onLoad("tabFuentes");
        onLoad("tabInfo");
        var jsonOficial = guardarDetalle("acordionDetallePrincipal_" + id, id);

        jsonOficial = JSON.stringify(jsonOficial)

        $.ajax({
            type: "POST",
            url: '@Url.Action("GuardarInformacionCaracteristicas", "Aire")',
            data: { jsonInfo: jsonOficial, idEstado: $("#txtidEstadoBase").val(), tblEstados: $("#txttblEstados").val() },
            beforeSend: function () { },
            success: function (response) {
                /*if (id == 0)
                    $('#btnGuardar2').attr('disabled', false);
                else if (id == 1)
                    $('#btnGuardar3').attr('disabled', false);*/

                if (response == "Ok") {
                    if (id == 0)
                        consultarJsonDetalle(0, "acordionDetalleGeneralChi", 0);
                    else
                        consultarJsonDetalleFuente(1, "acordionDetalleGeneralFuente", 1);

                    mensajeAlmacenamiento("Almacenamiento Exitoso");
                } else {
                    mensajeAlmacenamiento("Error al almacenar la informacion")
                }
                offLoad("tabFuentes");
                offLoad("tabInfo");

                setTimeout(function () { if (id == 0) $('#btnGuardar2').attr('disabled', false); else if (id == 1) $('#btnGuardar3').attr('disabled', false); }, 10000);
            }
        });
    }

    function CrearEstadoItemUni() {

        $.ajax({
            type: "POST",
            url: '@Url.Action("CrearEstadoItem", "Aire")',
            data: { idItem: $("#txtidItem").val(), idFormulario: $("#txtForm").val(), idVisita: $("#txtidVisita").val(), instalacion: $("#txtinstalacion").val(), tercero: $("#txttercero").val() },
            beforeSend: function () { },
            success: function (response) {
                var dato = eval('(' + response + ')');
                $("#txtidEstadoBase").val(dato)

                if (tipoV != "D") {
                    consultarJsonDetalle(0, "acordionDetalleGeneralChi", 0);
                    consultarJsonDetalleFuente(1, "acordionDetalleGeneralFuente", 1);
                    consultarJsonEncuesta()
                } else {
                    tipoV = "M";
                }
            }
        });
    }

    function GuardarUbicacion(num) {
        if (num == 1) {

            GuardarInformacionItem()
        } else if (num == 2) {

            GuardarInformacionCaracteristica(0);

        } else if (num == 3) {

            GuardarInformacionCaracteristica(1);

        } else if (num == 4) {
            guardarSistemaControl();

        } else if (num == 5) {


        }
    }
