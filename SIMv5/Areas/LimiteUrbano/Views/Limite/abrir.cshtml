function seleccionCoord(deciOSexa){
  if(deciOSexa == "sexagesimales"){
    gradX = chkValorNumerico($("#txtLongCoor").val()," Grados Longitud","#txtLongCoor");
    minX = chkValorNumerico($("#txtLongMin").val()," Minutos Longitud","#txtLongMin");
    secX = chkValorNumerico($("#txtLongSec").val()," Segundos Longitud","#txtLongSec");
    gradY = chkValorNumerico($("#txtLatCoor").val()," Grados Latitud","#txtLatCoor");
    minY = chkValorNumerico($("#txtLatMin").val()," Minutos Latitud","#txtLatMin");
    secY = chkValorNumerico($("#txtLatSec").val()," Segundos Latitud","#txtLatSec");
    coordX = (gradX + minX/60 + secX/3600)* parseFloat($("#cboEsteOeste option:selected").val());
    coordY = (gradY + minY/60 + secY/3600)* parseFloat($("#cboNorteSur option:selected").val());
  }
  if(deciOSexa == "decimales"){
    coordX = chkValorNumerico($("#txtXCoor").val()," X/Longitud","#txtXCoor");
    coordY = chkValorNumerico($("#txtYCoor").val()," Y/Latitud","#txtYCoor");
  }
}

function chkValorNumerico(valor,textoMensaje,idInput){
  var numero = 0.0;
  try {
      numero = parseFloat(valor);
      }
      catch(err) {
          alert("Aseg�rese de que en"+ textoMensaje +" el valor ingresado sea n�merico!");
          $(idInput).val("0.0");
      }
    return numero;
}



$("#cboSistemaRef").change(function(){
  valorSrid = $("#cboSistemaRef option:selected").val();
  tipoInput = "decimales"
  if(valorSrid != "-1"){
    tipoSistema = $("#cboSistemaRef option:selected").attr('label');
    for(index in objCoord){
      if(objCoord[index].SRID == valorSrid){
        transDatum = objCoord[index].TO_WGS84;
        break;
      }
    }

    if(tipoSistema == "PROYECTADO"){
      $("#divBtnOp").attr('hidden',true);
      $("#divDecimal").attr('hidden',false);
      $("#divSexagesimal").attr('hidden',true);
      $("#lblXCoor").text("X:");
      $("#lblYCoor").text("Y:");
    }else{
      $("#divBtnOp").attr('hidden',false);
      $("#divDecimal").attr('hidden',false);
      $("#divSexagesimal").attr('hidden',true);
        $("#rdbDegDec").attr('checked',true);
      $("#rdbDegSex").attr('checked',false);
      $("#rdbDegDec").trigger('click');


    }
  }else{
    $("#divDecimal").attr('hidden',true);
    $("#divSexagesimal").attr('hidden',true);
    $("#divBtnOp").attr('hidden',true); 
  }
});

$("input:radio[name=decOSex]").click(function(){
  tipoInput = $('input:radio[name=decOSex]:checked').val();
  if(tipoInput == "decimales"){
    $("#divDecimal").attr('hidden',false);
    $("#divSexagesimal").attr('hidden',true);
    
  }
  if(tipoInput == "sexagesimales"){
    $("#divDecimal").attr('hidden',true);
    $("#divSexagesimal").attr('hidden',false);
  }
});

function consultarcoordenadas(){
  if(valorSrid != "-1" ){
   
    //obtencion de las coordenadas de los inputs
    seleccionCoord(tipoInput);
  
    $.ajax({
      type:"POST",
      url:"/arcgis/rest/services/Utilities/Geometry/GeometryServer/project?inSR="+valorSrid+"&outSR="+wkidCoord+"&geometries="+coordX+"%2C"+coordY+"&transformation="+transDatum+"&transformForward=true&f=pjson",
    
      dataType:"json",
      success:function(result,status,xhr){
        //Verificacion de Geometria
        if(result.geometries != undefined){
          //verificacion de Coordenada
          if(result.geometries[0].x != undefined && result.geometries[0].x != "NaN" ){
            retornaDirecioninverso(result.geometries[0].x,result.geometries[0].y)
          
          }else{
         
            alert("Las coordenadas ingresadas se salen del marco de referencia.");
          }
        }else{
       
          alert("No se pudo construir una geometria a partir de las coordenadas ingresadas");
        }
      },
      error:function(xhr,status,err){
        
        alert("no fue posible comunicarse con el REST Geogr�fico");
      }
    });
  }
}


