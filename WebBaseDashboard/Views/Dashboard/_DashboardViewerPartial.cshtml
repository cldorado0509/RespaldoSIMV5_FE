@model DevExpress.DashboardWeb.Mvc.DashboardSourceModel
@Html.DevExpress().DashboardViewer(settings => {
	settings.Name = "DashboardViewer";
	settings.CallbackRouteValues = new { Controller = "Dashboard", Action = "DashboardViewerPartial" };
	settings.ExportRouteValues = new { Controller = "Dashboard", Action = "DashboardViewerPartialExport" };
    settings.FullscreenMode = true;
}).BindToSource(Model).GetHtml()